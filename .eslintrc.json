{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "no-unused-vars": ["warn", { "vars": "all", "varsIgnorePattern": "^_", "args": "after-used", "ignoreRestSiblings": true }],
    "@typescript-eslint/no-unused-vars": ["warn", { "vars": "all", "varsIgnorePattern": "^_", "args": "after-used", "ignoreRestSiblings": true }],
    "@typescript-eslint/no-explicit-any": "off",
     "@typescript-eslint/no-unused-expressions": "off"
  }
}
